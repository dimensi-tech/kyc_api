window.history.pushState('passport', 'Form Passport', '?identity_id=<%= @identity.nik %>');
$('html, body').animate({ scrollTop: 0 }, 'slow')
$('#form-holder > form').addClass('animated fadeOutLeft fast')
setTimeout(function(){
  $('#form-holder').html('<%= j render(partial: "passport") %>')
}, 800);

$("#new_passport").validate({
  rules: {
    'passport[nik]': {
      required: true
    },
    'passport[full_name]': {
      required: true
    },
    'passport[birth_place]': {
      required: true
    },
    'passport[mother_name]': {
      required: true
    },
    'passport[father_name]': {
      required: true
    },
    'passport[number]': {
      required: true
    },
    'passport[expired_date]': {
      required: true
    },
    'passport[released_date]': {
      required: true
    }
  }
})
